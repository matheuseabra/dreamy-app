{
  "name": "dreamy-studio",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "dev": "concurrently \"npm --workspace client run dev\" \"npm --workspace server run dev\"",
    "dev:client": "npm --workspace client run dev",
    "dev:server": "npm --workspace server run dev",
    "build": "npm --workspaces run build",
    "start": "npm --workspace server run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
